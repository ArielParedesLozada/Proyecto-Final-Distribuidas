{
  "ChoferService": {
    "Url": "localhost:5122"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http1AndHttp2"
    }
  },
  "eureka": {
    "client": {
      "serviceUrl": {
        "defaultZone": "http://localhost:8761/eureka"
      },
      "shouldRegisterWithEureka": true,
      "shouldFetchRegistry": true
    },
    "instance": {
      "appName": "auth-service",
      "hostName": "localhost",
      "port": "7037",
      "preferIpAddress": true,
      "metadataMap": {
        "httpPort": 5121,
        "grpcPort": 7037
      }
    }
  }
}